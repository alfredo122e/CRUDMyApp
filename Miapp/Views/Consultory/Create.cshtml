@model Miapp.Models.Consultory

@{
    ViewData["Title"] = "Nuevo Registro";
}
<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
<link rel="stylesheet" href="~/css/site.css" />

<div class="alert alert-info" role="alert">
    <div class="text-center">
<h1>Crear nuevo registro</h1>
</div></div>

<h4>Municipio</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="nombre" class="control-label"></label>
                <input asp-for="nombre" class="form-control" />
                <span asp-validation-for="nombre" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Enfermedad" class="control-label"></label>
                <input asp-for="Enfermedad" class="form-control" />
                <span asp-validation-for="Enfermedad" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Medicina" class="control-label"></label>
                <input asp-for="Medicina" class="form-control" />
                <span asp-validation-for="Medicina" class="text-danger"></span>
            </div>
           
            <div class="form-group">
                <input type="submit" value="Registrar" class="btn btn-primary" /> |
                <a class="btn btn-secondary" asp-action="Index" role="button">Regresar a la lista</a>
            </div>
        </form>
    </div>
</div>


